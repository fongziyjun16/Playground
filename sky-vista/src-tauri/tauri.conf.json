{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "npm run react-build",
        "beforeDevCommand": "npm run react-start",
        "devPath": "http://localhost:32828",
        "distDir": "../build"
    },
    "package": {
        "productName": "sky-vista"
    },
    "tauri": {
        "allowlist": {
            "window": {
                "all": false,
                "center": false,
                "close": false,
                "create": false,
                "hide": false,
                "maximize": false,
                "minimize": true,
                "print": false,
                "requestUserAttention": false,
                "setAlwaysOnTop": true,
                "setClosable": false,
                "setContentProtected": false,
                "setCursorGrab": false,
                "setCursorIcon": false,
                "setCursorPosition": false,
                "setCursorVisible": false,
                "setDecorations": false,
                "setFocus": false,
                "setFullscreen": false,
                "setIcon": false,
                "setIgnoreCursorEvents": false,
                "setMaxSize": false,
                "setMaximizable": false,
                "setMinSize": false,
                "setMinimizable": false,
                "setPosition": false,
                "setResizable": false,
                "setSize": false,
                "setSkipTaskbar": false,
                "setTitle": false,
                "show": false,
                "startDragging": true,
                "unmaximize": false,
                "unminimize": false
            }
        },
        "bundle": {
            "active": true,
            "category": "Utility",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
            "identifier": "fzy.sky-vista",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "systemTray": {
            "iconPath": "../app-icon.png",
            "iconAsTemplate": false
        },
        "updater": {
            "active": false
        },
        "windows": []
    }
}
